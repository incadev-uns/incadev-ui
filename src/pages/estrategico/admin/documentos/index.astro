---
import DocumentManagementPage from "@/process/strategic/dashboard/documents/DocumentManagementPage";
import PlanLayout from "@/process/strategic/layouts/PlanLayout.astro";
import "@/styles/global.css";

const title = "Dashboard - Gestion documentaria";
const description =
  "Modulo de gestion documentaria vinculada a objetivos estrategicos.";
const robots = ["noindex", "nofollow"];
const keywords = [
  "INCADEV",
  "estrategico",
  "documentos",
  "objetivos",
  "gestion",
];
const url =
  "https://instituto.cetivirgendelapuerta.com/estrategico/admin/documentos";
const imageOG = "/ISOLOGOTIPO_VERTICAL.svg";
const imagetwt = "/ISOLOGOTIPO_VERTICAL.svg";
---
<PlanLayout
  {robots}
  {title}
  {description}
  {keywords}
  {url}
  {imageOG}
  {imagetwt}
>
  <DocumentManagementPage client:load />
</PlanLayout>
