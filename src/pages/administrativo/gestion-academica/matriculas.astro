---
import DashboardLayout from '@/layouts/DashboardLayout.astro';
import EnrollmentManagement from '@/process/administrative/academic-management/Enrollment.tsx';

const robots = ["noindex", "nofollow"];
const title = "Matriculas | Incadev";
const description = "Gestión de matriculas para estudiantes en el Instituto";
const keywords = ["estudiantes", "gestión académica", "matriculas", "certificados", "instituto"];
const url = "https://instituto.cetivirgendelapuerta.com/administrativo/gestion-academica/matriculas";
const imageOG = "/ISOLOGOTIPO_VERTICAL.svg";
const imagetwt = "/ISOLOGOTIPO_VERTICAL.svg";
---

<DashboardLayout {robots} {title} {description} {keywords} {url} {imageOG} {imagetwt}>
	<EnrollmentManagement client:load />
</DashboardLayout>