---
import DashboardLayout from '@/layouts/DashboardLayout.astro';
import EnrollmentStatus from '@/process/administrative/academic-processes/EnrollmentStatus.tsx';
const robots = ["noindex", "nofollow"];
const title = "Estado de Matrículas | Incadev";
const description = "Seguimiento y gestión del estado académico y de pago de las matrículas";
const keywords = ["matrículas", "estudiantes", "estado académico", "pagos", "procesos académicos", "instituto"];
const url = "https://instituto.cetivirgendelapuerta.com/administrativo/procesos-academicos/enrollment-status";
const imageOG = "/ISOLOGOTIPO_VERTICAL.svg";
const imagetwt = "/ISOLOGOTIPO_VERTICAL.svg";
---

<DashboardLayout {robots} {title} {description} {keywords} {url} {imageOG} {imagetwt}>
<EnrollmentStatus client:load/>
</DashboardLayout>
