---
import PermissionsPage from '@/process/technology/dashboard/PermissionsPage';
import '@/styles/global.css';

const title = "Gestión de Permisos | INCADEV Tecnológico";
const description = "Administración de permisos del módulo tecnológico";
const robots = ["noindex", "nofollow"];
const keywords = ["INCADEV", "tecnológico", "permisos", "gestión"];
---

<!DOCTYPE html>
<html lang="es" class="dark">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>{title}</title>
  <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
</head>
<body class="min-h-screen bg-background font-sans antialiased">
  <PermissionsPage client:load />
</body>
</html>
