---
import DashboardLayout from '@/layouts/DashboardLayout.astro';
import AcademicConfig from '@/process/administrative/academic-processes/AcademicConfig';

const robots = ["noindex", "nofollow"];
const title = "Configuración Académica | Incadev";
const description = "Configuración Academica";
const keywords = ["estudiantes", "gestión académica", "matriculas", "certificados", "instituto"];
const url = "https://instituto.cetivirgendelapuerta.com/administrativo/academico/estudiantes";
const imageOG = "/ISOLOGOTIPO_VERTICAL.svg";
const imagetwt = "/ISOLOGOTIPO_VERTICAL.svg";
---

<DashboardLayout {robots} {title} {description} {keywords} {url} {imageOG} {imagetwt}>
	<AcademicConfig client:load />
</DashboardLayout>